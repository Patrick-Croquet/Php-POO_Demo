class Moi implements Humain{
    protected $add;
    protected $codepostal;
    protected $ville;
    
    public function setAdresse($cA, $cP, $cV){
        $this->add = $cA;
        $this->codepostal = $cP;
        $this->ville = $cV;
    }
    
    public function getAdresse(){
        return $this->add. '<br />' . $this->codepostal . '<br />' . $this->ville;
    }
}
